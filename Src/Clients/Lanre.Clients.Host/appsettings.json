{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.eShopOnContainers": "Information",
        "System": "Warning"
      }
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "Lanre": ""
  },
  "Database": {
    "UseInMemory": false,
    "Migrate": false,
    "EnsureDeleted": false
  },
  "Settings": {
    "DetailedErrors": true
  },
  "HttpsConfig": {
    "Port": 443
  }
}
